    "@sentry/node": "^5.14.2",
    "axios": "^0.19.2",
    "fastify": "^2.12.1",
    "fastify-favicon": "^2.0.0",
    "fastify-response-time": "^1.1.0",
    "fastify-static": "^4.7.0",
    "fastify-swagger": "^4.3.2",
