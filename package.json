{
  "name": "node-imgoptimize",
  "version": "0.4.5",
  "description": "Proxy server for image optimization on Node.JS use (fastify, axios, sharp)",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "set NODE_ENV=development&& nodemon app.js",
    "lint": "eslint --ignore-path .gitignore .",
    "lint-fix": "eslint --ignore-path .gitignore . --fix",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AntonLukichev/node-imgoptimize.git"
  },
  "keywords": [
    "node-js",
    "image-processing",
    "webp",
    "jpeg optimize",
    "fastify",
    "axios",
    "sharp",
    "libvips"
  ],
  "author": {
    "name": "Anton Lukichev",
    "email": "anton@lukichev.pro"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AntonLukichev/node-imgoptimize/issues"
  },
  "homepage": "https://github.com/AntonLukichev/node-imgoptimize#readme",
  "engines": {
